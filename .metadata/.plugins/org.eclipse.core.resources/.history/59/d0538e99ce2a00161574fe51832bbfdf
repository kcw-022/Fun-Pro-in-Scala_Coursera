package weektwo

object CH0205 {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  
  val d = new Rational (1,2)                      //> d  : weektwo.Rational = weektwo.Rational@b684286
  d.den                                           //> res0: Int = 3
}


class Rational(x: Int, y: Int) {
	def num = x
	def den = y
}